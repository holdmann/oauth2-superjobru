  },
  "require": {
    "league/oauth2-client": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Holdmann\\OAuth2\\Client\\Provider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Holdmann\\OAuth2\\Client\\Provider\\Tests\\": "tests/"
    }
  }
}
