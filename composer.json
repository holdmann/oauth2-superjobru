{
  "name": "holdmann/oauth2-superjobru",
  "description": "SuperJob.ru OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [{
    "name": "Oleg Sherbakov",
    "email": "holdmann@yandex.ru"
  }],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authentication",
    "superjob"
  ],
  "config": {
    "sort-packages": true
  },
  "scripts": {
